@misc{CMS-PAS-LUM-22-001,
	title = {Luminosity measurement in proton-proton collisions at 13.6{\textbackslash},{\textbackslash}{mathrmTeV} in 2022 at {CMS}},
	url = {https://cds.cern.ch/record/2890833},
	abstract = {The measurement of the integrated luminosity for the proton-proton collisions data-taking period at a center-of-mass energy of \$13.6{\textbackslash},{\textbackslash}mathrm\{TeV\}\$ in 2022 with the CMS experiment at the CERN LHC is reported. The absolute scale of the luminosity measurement is calibrated from beam-separation scans with the van der Meer scan method. The precision of the calibration is limited by the knowledge of the factorization of the bunch proton density during the van der Meer scans. Continuous rate measurements with various CMS subdetectors provide a stable and linear luminosity measurement. Considering both calibration and integration sources, the integrated luminosity measurement has a total uncertainty of \$1.4{\textbackslash}\%\$.},
	urldate = {2024-11-27},
	publisher = {CERN},
	year = {2024},
}


@misc{Alici:1427728,
	title = {Study of the {LHC} ghost charge and satellite bunches for luminosity calibration.},
	url = {https://cds.cern.ch/record/1427728},
	abstract = {An important aspect of luminosity calibration measurements is the bunch population product normalization. In the case of the LHC, the treatment of this normalization can be split into three subjects: the total current measurement, the relative amplitude of the individual bunch populations and the corrections from the non-perfect longitudinal distribution. In this note, the last item is discussed in details and in the context of the 2010 and 2011 luminosity calibration measurements performed for each LHC Interaction Point. The different methods for quantifying the ghost charge and satellite populations are described and their uncertainties reviewed.},
	urldate = {2024-11-27},
	author = {Alici, A and Anders, G and Bacchetta, N and Balagura, V and Barschel, C and Bravin, E and Burkhardt, H and Ferro-Luzzi, M and Gagliardi, M and Gras, J J and Heinemann, B and Hopchev, P and Huhtinen, M and Jeff, A and Kozanecki, W and Pagan Griso, S and Panman, J and White, S and Zuranski, A},
